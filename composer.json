{
    "name": "custom/event-registration-module",
    "description": "Drupal 10 Event Registration Module",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Developer",
            "email": "developer@example.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "drupal/core": "^10.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true
        }
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        }
    }
}
